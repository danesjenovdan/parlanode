{
  "name": "parlassets",
  "version": "1.0.0",
  "description": "Assets for frontend for Parlameter",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass scss/style.scss scss/style.css && postcss -u autoprefixer -o scss/style.css scss/style.css",
    "dev": "nodemon --ext scss --exec \"yarn run sass\"",
    "serve": "http-server -p 8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danesjenovdan/parlassets.git"
  },
  "author": "danesjenovdan",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/danesjenovdan/parlassets/issues"
  },
  "homepage": "https://github.com/danesjenovdan/parlassets#readme",
  "dependencies": {
    "autoprefixer": "^7.1.1",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.0.0",
    "css-loader": "^0.28.4",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.3.0",
    "file-loader": "^0.11.1",
    "http-server": "^0.11.1",
    "node-sass": "^4.5.3",
    "nodemon": "^1.18.4",
    "postcss-cli": "^4.0.0",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^2.6.1",
    "webpack-dev-server": "^2.4.5"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ]
  },
  "eslintConfig": {
    "plugins": [
      "html"
    ],
    "extends": "airbnb-base"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "devDependencies": {
    "gulp": "3.9.1",
    "gulp-clean-css": "3.4.1",
    "gulp-rename": "1.2.2",
    "gulp-uglify": "3.0.0",
    "pump": "1.0.2"
  }
}
