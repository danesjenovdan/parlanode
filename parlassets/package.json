{
  "name": "parlassets",
  "version": "1.0.0",
  "description": "Assets for frontend for Parlameter",
  "main": "index.js",
  "scripts": {
    "sass": "node-sass --functions=scss/functions.js scss/style.scss scss/style.css && postcss -u autoprefixer -o scss/style.css scss/style.css",
    "dev": "cross-env NODE_ENV=development nodemon --ext scss --exec \"npm run sass\"",
    "serve": "http-server -p 8080"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danesjenovdan/parlassets.git"
  },
  "author": "danesjenovdan",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/danesjenovdan/parlassets/issues"
  },
  "homepage": "https://github.com/danesjenovdan/parlassets#readme",
  "dependencies": {
    "autoprefixer": "^9.4.2",
    "cross-env": "^5.0.0",
    "http-server": "^0.11.1",
    "lodash": "^4.17.11",
    "node-sass": "^4.5.3",
    "nodemon": "^1.18.4",
    "postcss-cli": "^6.0.1",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "eslintConfig": {
    "plugins": [
      "html"
    ],
    "extends": "airbnb-base"
  },
  "stylelint": {
    "extends": "stylelint-config-standard"
  },
  "devDependencies": {}
}
