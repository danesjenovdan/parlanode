{
  "name": "parlanode",
  "version": "1.0.0",
  "description": "Parlameter frontend server for serving embeds",
  "main": "run.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu pm2 start run.js --name=parlanode",
    "dev": "cross-env NODE_ENV=development NODE_ICU_DATA=node_modules/full-icu nodemon run.js",
    "cards-generate": "node cards/generate.js",
    "cards": "node cards/build-cross-env.js",
    "og-images": "node og-images/build.js",
    "lint": "eslint . --ext js,vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danesjenovdan/parlanode.git"
  },
  "author": "danesjenovdan",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/danesjenovdan/parlanode/issues"
  },
  "homepage": "https://github.com/danesjenovdan/parlanode#readme",
  "dependencies": {
    "axios": "0.18.0",
    "babel-core": "6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-lodash": "3.3.4",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "1.7.0",
    "body-parser": "1.18.3",
    "chalk": "2.4.1",
    "cors": "^2.8.4",
    "cron": "^1.3.0",
    "cross-env": "5.2.0",
    "cross-spawn": "^6.0.5",
    "css-loader": "0.28.11",
    "date-fns": "^1.29.0",
    "eslint": "4.19.1",
    "eslint-config-airbnb-base": "13.0.0",
    "eslint-import-resolver-alias": "1.1.1",
    "eslint-plugin-import": "2.13.0",
    "eslint-plugin-vue": "4.7.1",
    "express": "4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fs-extra": "^7.0.0",
    "full-icu": "1.2.1",
    "glob": "^7.1.2",
    "handlebars": "4.0.11",
    "inquirer": "6.0.0",
    "lodash": "4.17.10",
    "lodash-webpack-plugin": "^0.11.5",
    "mongoose": "^5.2.1",
    "node-fetch": "^2.2.0",
    "node-sass": "^4.9.1",
    "nodemon": "1.18.3",
    "puppeteer": "^1.6.1",
    "sass-loader": "^7.0.3",
    "tree-kill": "^1.2.0",
    "vue": "^2.5.16",
    "vue-i18n": "7.8.1",
    "vue-i18n-extensions": "^0.1.0",
    "vue-loader": "^15.2.4",
    "vue-pell-editor": "^2.0.1",
    "vue-server-renderer": "^2.5.16",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.15.1",
    "webpack-cli": "3.1.0",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.3",
    "webpack-node-externals": "^1.7.2"
  },
  "babel": {
    "plugins": [
      "lodash",
      "transform-object-rest-spread",
      [
        "transform-runtime",
        {
          "polyfill": false,
          "regenerator": true
        }
      ]
    ],
    "presets": [
      "env"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "data/*",
      "cards/*",
      "og-images/*",
      "og/*"
    ]
  }
}
