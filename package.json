{
  "name": "parlanode",
  "version": "1.0.0",
  "description": "Parlameter frontend server for serving embeds",
  "main": "run.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production NODE_ICU_DATA=node_modules/full-icu pm2 start run.js --name=parlanode",
    "dev": "cross-env NODE_ENV=development NODE_ICU_DATA=node_modules/full-icu nodemon run.js",
    "cards-generate": "node cards/generate.js",
    "cards": "cross-env NODE_ENV=development node cards/build-cross-env.js",
    "og-images": "cross-env NODE_ENV=development node og-images/build.js",
    "lint": "eslint . --ext js,vue"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danesjenovdan/parlanode.git"
  },
  "author": "danesjenovdan",
  "license": "Unlicense",
  "bugs": {
    "url": "https://github.com/danesjenovdan/parlanode/issues"
  },
  "homepage": "https://github.com/danesjenovdan/parlanode#readme",
  "dependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
    "@babel/runtime": "^7.8.3",
    "axios": "^0.19.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-lodash": "^3.3.4",
    "body-parser": "^1.19.0",
    "chalk": "^3.0.0",
    "cors": "^2.8.4",
    "cron": "^1.7.2",
    "cross-env": "^6.0.3",
    "cross-spawn": "^7.0.1",
    "css-loader": "^3.4.2",
    "d3v3": "^1.0.3",
    "date-fns": "^2.9.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-import-resolver-alias": "^1.1.1",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-vue": "^4.7.1",
    "express": "^4.17.1",
    "fs-extra": "^8.1.0",
    "full-icu": "^1.3.0",
    "glob": "^7.1.6",
    "handlebars": "^4.7.2",
    "inquirer": "^7.0.3",
    "leaflet": "^1.6.0",
    "lodash": "^4.17.21",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.9.0",
    "mongoose": "^5.8.7",
    "node-fetch": "^2.6.0",
    "node-sass": "^4.13.1",
    "nodemon": "^2.0.2",
    "papaparse": "^5.1.1",
    "puppeteer": "^2.0.0",
    "raw-loader": "^4.0.0",
    "sass-loader": "^8.0.2",
    "stable": "^0.1.8",
    "tree-kill": "^1.2.2",
    "vue": "^2.6.11",
    "vue-i18n": "^8.15.3",
    "vue-i18n-extensions": "^0.2.1",
    "vue-loader": "^15.8.3",
    "vue-pell-editor": "^2.0.1",
    "vue-server-renderer": "^2.6.11",
    "vue-template-compiler": "^2.6.11",
    "vue2-leaflet": "^2.4.2",
    "webpack": "^4.41.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1",
    "webpack-merge": "^4.2.2",
    "webpack-node-externals": "^1.7.2"
  },
  "babel": {
    "plugins": [
      "lodash",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-transform-runtime"
    ],
    "presets": [
      "@babel/preset-env"
    ]
  },
  "nodemonConfig": {
    "ignore": [
      "data/*",
      "cards/*",
      "og-images/*",
      "og/*"
    ]
  }
}
