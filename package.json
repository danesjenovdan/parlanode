{
  "name": "parlanode",
  "version": "0.0.0",
  "author": "Danes je nov dan",
  "license": "Unlicense",
  "type": "module",
  "scripts": {
    "dev": "vite --config build/vite.config.dev.js",
    "build": "yarn build:client && yarn build:server && yarn build:locales",
    "build:client": "vite build --config build/vite.config.build.js --ssrManifest --outDir dist/client",
    "build:server": "SSR=1 vite build --config build/vite.config.build.js --ssr --outDir dist/server",
    "build:locales": "node build/build-locales.js",
    "start": "node server/index.js",
    "lint": "yarn lint:eslint && yarn lint:card-names && yarn lint:locale-names",
    "lint:eslint": "eslint . --ext js,cjs,mjs,vue || exit 0",
    "lint:card-names": "node build/lint-card-names.js",
    "lint:locale-names": "node build/lint-locale-names.js"
  },
  "dependencies": {
    "@intlify/vite-plugin-vue-i18n": "^2.1.2",
    "@intlify/vue-i18n-extensions": "^2.0.1",
    "@rollup/plugin-virtual": "^2.0.3",
    "@rollup/plugin-yaml": "^3.0.0",
    "@vitejs/plugin-vue": "^1.1.5",
    "@vue/compiler-sfc": "^3.0.11",
    "@vue/server-renderer": "^3.0.11",
    "axios": "^0.21.2",
    "d3": "^6.6.2",
    "date-fns": "^2.9.0",
    "dotenv-expand": "^5.1.0",
    "dotenv-flow": "^3.2.0",
    "fastify": "^3.14.0",
    "fastify-cors": "^6.0.1",
    "fastify-static": "^4.2.4",
    "fs-extra": "^9.1.0",
    "html-safe-json": "^1.0.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "make-promises-safe": "^5.1.0",
    "rollup": "^2.42.4",
    "sass": "^1.32.8",
    "vite": "^2.3.5",
    "vue": "^3.0.11",
    "vue-i18n": "^9.1.4",
    "vue-pell-editor": "^2.0.1"
  },
  "devDependencies": {
    "@types/d3": "^6.3.0",
    "@types/lodash-es": "^4.17.4",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.8.0",
    "glob": "^7.1.6",
    "js-yaml": "^4.1.0",
    "prettier": "^2.2.1",
    "remarkable": "^2.0.1"
  }
}
