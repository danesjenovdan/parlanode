{
  "name": "parlanode",
  "version": "0.0.0",
  "author": "Danes je nov dan",
  "license": "Unlicense",
  "type": "module",
  "scripts": {
    "dev": "vite --config build/vite.config.dev.js",
    "build": "yarn build:client && yarn build:server && yarn build:locales",
    "build:client": "vite build --config build/vite.config.build.js --ssrManifest --outDir dist/client",
    "build:server": "SSR=1 vite build --config build/vite.config.build.js --ssr --outDir dist/server",
    "build:locales": "node build/build-locales.js",
    "start": "node server/index.js",
    "start:watch": "nodemon -e js,cjs,mjs,vue,html server/index.js",
    "lint": "yarn lint:eslint && yarn lint:card-names && yarn lint:locale-names && yarn lint:locale-keys",
    "lint:eslint": "eslint . --ext js,cjs,mjs,vue",
    "lint:card-names": "node build/lint-card-names.js",
    "lint:locale-names": "node build/lint-locale-names.js",
    "lint:locale-keys": "node build/lint-locale-keys.js"
  },
  "dependencies": {
    "@intlify/vite-plugin-vue-i18n": "^7.0.0",
    "@intlify/vue-i18n-extensions": "^5.0.1",
    "@rollup/plugin-virtual": "^2.0.3",
    "@rollup/plugin-yaml": "^3.1.0",
    "@sentry/tracing": "^7.38.0",
    "@sentry/vue": "^7.38.0",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/server-renderer": "3.2.38",
    "axios": "^0.26.1",
    "d3": "^6.7.0",
    "date-fns": "^2.25.0",
    "fastify": "^3.29.4",
    "fastify-cors": "^6.0.3",
    "fastify-static": "^4.6.1",
    "fs-extra": "^10.0.0",
    "html-safe-json": "^1.0.2",
    "lodash-es": "^4.17.21",
    "make-promises-safe": "^5.1.0",
    "recaptcha-v3": "^1.10.0",
    "remarkable": "^2.0.1",
    "rollup": "^2.58.0",
    "sass": "^1.43.2",
    "vite": "^3.2.7",
    "vue": "3.2.38",
    "vue-i18n": "^9.2.2"
  },
  "devDependencies": {
    "@types/d3": "^6.7.5",
    "@types/lodash-es": "^4.17.4",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.5.0",
    "glob": "^7.2.0",
    "js-yaml": "^4.1.0",
    "nodemon": "^2.0.14",
    "prettier": "^2.4.1"
  }
}
