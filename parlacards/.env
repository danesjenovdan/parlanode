# self base url
VITE_PARLACARDS_URL=http://localhost:7004

# main site url
VITE_PARLASITE_URL=https://parlasite.lb.djnd.si

# meta image generator url
VITE_METAIMAGES_URL=https://meta-image-generator.lb.djnd.si/parlameter

# production server port
PORT=7004
