<% include ../../partials/header.ejs %>

<div class="group-container">
    <div class="row header  text-center">
        <div class="col-lg-12">

            <div class="media-object img-circle session-logo <%- ps.acronym %>-background  <%- ps.acronym %>-fill  <%- ps.acronym %>-border ">
                <b><%- ps.acronym %></b>
            </div>

            <h1><%- ps.name %></h1>

            <font>
                <%=(ps.is_coalition)?"koalicija":"opozicija"%>
            </font>
            <div class="group-container-menu">
                <ul class="clearfix">
                    <li>
                        <a class="active" href="/poslanska-skupina/pregled/<%- slug %>">Pregled</a>
                    </li>
                    <li>
                        <a href="/poslanska-skupina/glasovanja/<%- slug %>">Glasovanja</a>
                    </li>
                    <li>
                        <a href="/poslanska-skupina/govori/<%- slug %>">Govori</a>
                    </li>
                </ul>
            </div>

        </div>
    </div>
</div>


<div class="col-md-6 ">
    <%- views.osnovneInformacije %>
</div>
<div class="col-md-6 ">
    <%- views.claniceInClani %>
</div>
<div class="col-md-12 ">
    <%- views.kompas %>
</div>
<div class="col-md-6 ">
    <%- views.izracunanaPrisotnostSeje %>
</div>
<div class="col-md-6 ">
    <%- views.izracunanaPrisotnostGlasovanja %>
</div>

<% include ../../partials/footer.ejs %>
