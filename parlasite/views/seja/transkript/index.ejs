<% include ../../partials/header.ejs %>
<%if(typeof sesData !== 'undefined'){%>

<div class="session-container">
    <div class="row header text-center">
        <div class="col-lg-12">

            <% if(typeof sesData !== "undefined" ){%>
            <div class="media-object img-circle session-logo">
                <img src="https://cdn.parlameter.si/v1/parlassets/icons/seja-<%= sesData.name.split(' ')[1] %>.svg" alt="<%-sesData.name%>">
            </div>

            <h1> <%-sesData.name%></h1>
            <h2> <%-sesData.org.name%></h2>

            <font class="date">
                Začetek seje: <%-sesData.date%> <%=(sesData.in_review)?'(v pregledu)':null%>
            </font>
            <br/>
            <font class="datepublished">
                Podatki osveženi: <%-sesData.updated_at%>
            </font>

            <% } %>

            <% //if((sesData.type != 'kolegij') && (sesData.votes == true)) { %>
            <div class="group-container-menu">
                <ul class="clearfix">
                    <li>
                        <a href="/seja/zakonodaja/<%- params.id %>">
                            Zakonodaja
                        </a>
                    </li>
                    <li>
                        <a href="/seja/druga-glasovanja/<%- params.id %>">
                            Druga glasovanja
                        </a>
                    </li>
                    <li>
                        <a href="/seja/transkript/<%- params.id %>" class="active">
                            Transkript
                        </a>
                    </li>
                </ul>
            </div>
            <% //} %>

        </div>
    </div>
</div>


<% if(sesData.speeches){ %>

<div class="row">
    <div class="col-md-12 ">
        <% if (sesData.type != 'kolegij') { %>
        <%- views.besedeKiSoZaznamovaleSejo %>
        <%}%>
    </div>
</div>

<div class="row">
    <div class="col-md-12">
        <%- views.govori %>
    </div>
</div>

<% }else{ %>
<%include ../../empty-state/state5.ejs %>
<% } %>

<%}else{ %>
    <%include ../../error/404partial.ejs %>
<%}%>
<% include ../../partials/footer.ejs %>
