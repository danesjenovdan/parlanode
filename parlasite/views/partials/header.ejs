<script>

    function equalHeight($elementByClass) {

        var h = 0;
        $($elementByClass).each(function () {
            if($(this).height() > h){
                h = $(this).height();
            }
        });
        $($elementByClass).height(h);

    }

    $(function () {

        $(window).resize(function(){
            equalHeight(".session_type");
            equalHeight(".session_types ul li");
        });

        if($(".session_type").length > 0){
            equalHeight(".session_type");
            equalHeight(".session_types ul li");
        }

    })

</script>

