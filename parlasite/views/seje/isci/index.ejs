        <div class="narrow-inner-container">
          <h2><%- i18n('landing.search.part_one'); %> </h2>
          <div id="about">
            <p class="public-col">
                <%- i18n('landing.search.part_two'); %>
            </p>
          </div>
          <div class="search-container">
            <%- await fetchCard('/c/search/'); %>
          </div>
          <div class="searchinfo measure" data-mc="search" data-ma="infobox" data-mn="" data-mv="" data-toggle="modal" data-target="#modal-search-how">i</div>
        </div>
      </div>
    </div>
  </div>
  <% include ../../_partials/search-popup.ejs %>
</div>

<style>
  .searchinfo {
    top: 118px;
  }
</style>

<div id="session-search-results">
  <div class="col-md-12">
    <%- await fetchCard('/s/nastopi-v-katerih-je-bil-iskalni-niz-izrecen/', { state: { query } }) %>
  </div>
  <div class="col-md-12">
    <%- await fetchCard('/s/zakonodaja-iskanje/', { state: { query } }) %>
  </div>
  <div class="col-md-12">
    <%- await fetchCard('/s/raba-skozi-cas/', { state: { query } }) %>
  </div>
  <div class="col-md-6">
    <%- await fetchCard('/s/najveckrat-so-pojem-uporabili/', { state: { query } }) %>
  </div>
  <div class="col-md-6">
    <%- await fetchCard('/s/raba-po-strankah/', { state: { query } }) %>
  </div>
  <div class="col-md-12">
    <%- await fetchCard('/s/glasovanja-search/', { state: { query } }) %>
  </div>
</div>

<% include ../../_partials/footer.ejs %>
